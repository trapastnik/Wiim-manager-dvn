# WiiM Mini IP address in your local network
WIIM_IP=192.168.1.xxx

# Port for this web interface
PORT=3000

# WiiM uses HTTPS with self-signed certificate
WIIM_USE_HTTPS=true

# Request timeout in milliseconds (default: 5000)
# Увеличьте при медленном WiFi, уменьшите для быстрой сети
WIIM_REQUEST_TIMEOUT=5000

# Enable server statistics tracking (default: false)
# Отключите для максимальной производительности
ENABLE_STATS=false
